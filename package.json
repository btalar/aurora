{
  "name": "aurora-components-lib",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/components/main.js",
  "module": "./dist/components/main.js",
  "types": "./dist/components/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/components/main.js",
      "types": "./dist/components/main.d.ts"
    },
    "./components": {
      "import": "./dist/components/main.js",
      "types": "./dist/components/main.d.ts"
    },
    "./components/*": {
      "import": "./dist/components/*",
      "types": "./dist/components/*.d.ts"
    }
  },
  "scripts": {
    "dev": "vite --config vite.config.playground.ts",
    "playground": "vite --config vite.config.playground.ts",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build-playground": "vite build --config vite.config.playground.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "slint": "npx stylelint \"**/*.css\"",
    "ncui": "npx npm-check-updates -i",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@storybook/csf": "^0.1.13",
    "classnames": "2.5.1",
    "react-aria-components": "1.14.0"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "2.3.12",
    "@eslint/js": "9.39.1",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@stylistic/eslint-plugin": "5.6.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "16.3.0",
    "@types/eslint-plugin-jsx-a11y": "6.10.1",
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "@vitest/coverage-v8": "4.0.15",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-dom": "2.3.12",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-plugin-storybook": "10.1.11",
    "glob": "13.0.0",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "jsdom": "27.2.0",
    "lint-staged": "16.2.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "rollup-preserve-directives": "1.1.3",
    "storybook": "10.1.11",
    "storybook-design-token": "^5.0.0",
    "stylelint": "16.26.1",
    "stylelint-config-standard": "39.0.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "8.48.1",
    "vite": "npm:rolldown-vite@7.2.10",
    "vite-plugin-dts": "4.5.4",
    "vite-plugin-eslint2": "5.0.4",
    "vite-plugin-lib-inject-css": "2.2.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.15"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.10"
  }
}
